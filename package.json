{
    "name": "dbc-snippets",
    "publisher": "David-Garcia",
    "displayName": "dbc-snippets",
    "description": "Snippets and Syntax Highlighting for DBC",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.53.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dgghun/dbc-snippets.git"
    },
    "categories": ["Programming Languages", "Snippets"],
    "contributes": {
        "languages": [
            {
                "id": "dbc",
                "aliases": ["DBC", "dbc"],
                "extensions": [".DGG",".TXT",".DEF",".VRB",".DAV",".GLE",".GWS",".AJS",".MAG",".DO"],
                "configuration": "./language-configuration.json"
            }
          ],
          "grammars": [
            {
                "language": "dbc",
                "scopeName": "DBC",
                "path": "./syntaxes/dbc.tmLanguage.json"
            }
          ],
        "snippets": [
            {
                "language": "dbc",
                "aliases": ["DBC", "dbc"],
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
